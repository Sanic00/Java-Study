package EventEx;

import java.awt.*;
import java.awt.event.*;

public class WindowEventEx02 extends Frame implements WindowListener{

	public WindowEventEx02() {
		super("WinEvent2");
		addWindowListener(this);
		setSize(300, 200);
		setVisible(true);
	}
	
	
	public static void main(String[] args) {
	
		new WindowEventEx02();
	}

	
	
	
	
	
	
	
	
	
	
}
